{
  "name": "magic-tournament-season-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/db/data-source.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/db/data-source.ts",
    "schema:synk": "npm run typeorm schema:synk -- -d ./src/db/data-source.ts",
    "migration:show": "npm run typeorm migration:show -- -d ./src/db/data-source.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/db/data-source.ts",
    "migration:create": "npm run typeorm migration:generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "express": "^4.19.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.13.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
